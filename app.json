{
  "expo": {
    "name": "FITNEE_Client_Refactor",
    "slug": "FITNEE_Client_Refactor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/PNGs/AppIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/PNGs/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F8FA"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/PNGs/Splash.png",
        "backgroundColor": "#F6F8FA"
      }
    },
    "web": {
      "favicon": "./src/assets/images/PNGs/AppIcon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./src/assets/fonts/Pretendard-Black.otf"]
        }
      ]
    ]
  }
}
